	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug",
			"type": "gdb",
			"request": "launch",
			"target": "./dsqllint",
			"arguments": "tests/res",
			"cwd": "${workspaceRoot}",
			"valuesFormatting": "parseText",
			"preLaunchTask": "build"
		},
		{
			"name": "Test debug",
			"type": "gdb",
			"request": "launch",
			"target": ".out/dsqllint-test-unittest",
			"arguments": "",
			"cwd": "${workspaceRoot}",
			"valuesFormatting": "parseText"
		}
	]
}
